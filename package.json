{
    "name": "bugsnag",
    "description": "Bugsnag notifier for node.js scripts",
    "version": "1.12.2",
    "main": "./lib/bugsnag.js",
    "typings": "./lib/bugsnag.d.ts",
    "homepage": "http://bugsnag.com",
    "dependencies": {
        "promise": "7.x",
        "stack-trace": "~0.0.9",
        "request": "^2.81.0",
        "json-stringify-safe": "~5.0.1"
    },
    "devDependencies": {
        "chai": "~1.5.0",
        "coveralls": "^2.13.1",
        "express": "~3.4.2",
        "grunt": "~0.4.0",
        "grunt-bumpx": "latest",
        "mocha": "latest",
        "nyc": "^11.2.1",
        "sinon": "latest"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:bugsnag/bugsnag-node.git"
    },
    "keywords": [
        "error",
        "bugsnag",
        "exception"
    ],
    "license": "MIT",
    "scripts": {
        "test": "nyc --reporter=lcov --reporter=text mocha && (if [ -n \"$TRAVIS\" ]; then cat ./coverage/lcov.info | coveralls; fi)",
        "test:quick": "mocha"
    },
    "engine": {
        "node": ">=4"
    }
}
